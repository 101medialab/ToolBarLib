<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SearchView">
        <attr name="hksearchview_layout" format="reference" />
        <attr name="hksearchview_iconifiedByDefault" format="boolean" />
        <attr name="android:maxWidth" />
        <attr name="hksearchview_queryHint" format="string" />
        <attr name="android:imeOptions" />
        <attr name="android:inputType" />
        <attr name="android:focusable" />
    </declare-styleable>

    <declare-styleable name="SearchActionBar">
        <attr name="hkmsab_closeIcon" format="reference" />
        <attr name="hkmsab_goIcon" format="reference" />
        <attr name="hkmsab_searchIcon" format="reference" />
        <attr name="hkmsab_searchHintIcon" format="reference" />
        <attr name="hkmsab_voiceIcon" format="reference" />
        <attr name="hkmsab_commitIcon" format="reference" />
        <attr name="hkmsab_suggestionRowLayout" format="reference" />
        <attr name="hkmsab_queryBackground" format="reference" />
        <attr name="hkmsab_submitBackground" format="reference" />
        <attr name="hkmsab_layout" format="reference" />
    </declare-styleable>

    <declare-styleable name="MaterialSearchView">
        <attr name="searchOverlayColor" format="color" />
        <attr name="searchBackground" format="integer" />
        <attr name="searchVoiceIcon" format="integer" />
        <attr name="searchCloseIcon" format="integer" />
        <attr name="searchBackIcon" format="integer" />
        <attr name="searchSuggestionBackground" format="integer" />
        <attr name="android:hint" />
        <attr name="android:textColor" />
        <attr name="android:textColorHint" />
        <attr name="material_layout" format="enum">
            <enum name="DEFAULT" value="0" />
            <enum name="POPBEE" value="1" />
            <enum name="HB" value="2" />
        </attr>
    </declare-styleable>
</resources>